// Top Section 100vh
#hero-section.uk-section
    #hero.uk-background-cover.uk-flex.uk-flex-middle(uk-parallax='bgy: -100;' style="background-image: url('images/rootedbg.jpg')" uk-height-viewport)
        .uk-container.uk-text-center
            div.uk-light
                h1 Rooted Psychotherapy &amp; Counseling
                h2 Quality Care You Can Count On
            div
                a.uk-button.uk-button-default(href="#" uk-toggle="target: #modal-full") Contact Us Today
            div.uk-dark
                a.uk-button.uk-button-primary.uk-margin-top(href="blog/forms-available-for-download") New Client Forms    
            .uk-margin-top.uk-light
                a(href="#abouts" uk-scroll)
                    i.bounce(uk-icon="icon: chevron-down; ratio: 3" uk-parallax="opacity: 1,0; viewport: 0.7;")